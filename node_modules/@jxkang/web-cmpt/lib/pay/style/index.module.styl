.title
  font-weight: bold
  padding-bottom: 5px

.my_table
  width:100%
  th
    text-align: right
    padding-right:10px
    width:32%
  td,th
    padding-top:7px
    padding-bottom:7px

.payUrl
  display: inline-block
  width:310px
  color:#666
  white-space: nowrap
  overflow hidden
  height:25px
  line-height: 25px
  vertical-align: middle
  border-radius: 3px

.copy_btn
  margin-left:7px
  vertical-align: middle
  cursor pointer
  background-color: #f7f7f7
  border:1px solid #ccc
  display:inline-block
  height:25px
  line-height: 25px
  padding-left:5px
  padding-right:5px
  border-radius: 3px

.bank_pay_pop
  :global .ant-modal-body
    padding: 0

.enabled_amount
  position: absolute
  left:15px
  bottom:15px
  color:#595959