.busLib
  width 80%
  margin 0 auto
  display flex
  justify-content center
.inner
  width 70%
  overflow hidden
  padding 30px 0
.topInner
  font-size 30px
  color #000
  font-weight 500
  text-align center
  margin-bottom 8px
.disInner
  text-align center
  font-size 16px
  color rgba(0, 0, 0, 0.45)
  margin-bottom 36px
.formStyle
  width 100%
  display flex
  flex-direction column
  align-items center
  :global
    .ant-form-item
      width 60%
.innerText
  font-size 12px
  color #999999
  line-height 18px
.picBox
  display flex
.pic
  display inline-block
  width 102px
  height 102px
  border-radius 4px
  border 1px solid #ccc
  margin 0 10px 0 0