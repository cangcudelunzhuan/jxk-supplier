.contentInner
  width 100%
  background rgba(252,250,251,1);
.goods_content
  width:100%
  background:#fff
  border-radius: 4px
  border-bottom-left-radius: none;
  border-bottom-right-radius: none;
  display: flex
  justify-content: center
  padding: 30px 0
  :global
    .ant-form
      width:100%
      display: flex
      flex-direction: column
      align-items:center
    .ant-form-item
       width:700px
.tableInner
  :global
    .ant-form-item
       width:auto
.add_btn
  width: 100%
  color: $theme-color
  font-size: 14px
  display: flex
  justify-content: center
  align-items: center
  cursor pointer
.innerForm
  padding-left:8%
.logo_upload
  display: flex
  align-items: center
.logo_upload
  :global
    .ant-upload-list-picture .ant-upload-list-item, .ant-upload-list-picture-card .ant-upload-list-item
      padding: 6px
.avatar_uploader
  width auto
.discript
  display: flex
  align-items: center
  justify-content: center
.innerForm
 margin-bottom 10px
.uploadText 
  margin-left 10px
  font-size: 14px 
  color: $theme-color
  cursor pointer
  margin-top:16px
.tableInner
  display: grid
  grid-template-columns: 1fr 1fr;
  border:1px solid rgba(217,217,217,1);
  width:280px
  border-radius: 4px
  margin-top:10px
  margin-bottom: 10px
  >div
  //  height: 45px
   display: flex
   justify-content: center
   align-items: center
.topInner
  background:#F0F2F5
  border-right:1px solid rgba(217,217,217,1);
  border-bottom:1px solid rgba(217,217,217,1);
  border-top-left-radius: 4px
  padding:10px 0
.botInner
  background:#F0F2F5
  border-bottom:1px solid rgba(217,217,217,1);
  border-top-right-radius: 4px
  padding:10px 0
.rightInner
   border-right:1px solid rgba(217,217,217,1);
   width:'60%'
.iconStyle
  display: flex
  margin-right:10px
  align-items: center
.editorStyle
  color:$theme-color
  cursor:pointer
.innerBox
  display: flex
  justify-content:space-between
  :global
    .ant-form-item
       width:auto

