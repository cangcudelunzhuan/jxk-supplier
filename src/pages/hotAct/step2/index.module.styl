
.card_box
  :global
    .ant-card-head
      padding: 0 15px
      min-height: 35px
      border-bottom: 0px
    .ant-card-head-title
      padding: 16px 13px
      font-size: 14px
      line-height: 18px
      position: relative
    .ant-card-head-title:after
      position: absolute
      height :14px
      top: 0
      left:0
      bottom: 0
      margin: auto 0
      border-left: 4px solid $theme-color
      z-index: 3
      content: ''
    .ant-card-body
      padding: 0 15px 20px 15px
.card_box+.card_box
  margin-top: 0
.card_allbox
  :global
    .ant-tabs-nav .ant-tabs-tab
      padding: 9px
      margin-right: 10px 
    .ant-tabs-nav .ant-tabs-tab-active
      border: 1px solid #d9d9d9
      border-radius: 4px
      border-bottom: 0
      position: relative
      color: $theme-color
    .ant-tabs-nav .ant-tabs-tab-active:after 
      content :''
      position: absolute
      left:0
      width: 100%
      height:1px
      background: #fff
      bottom: 1px
    .ant-tabs-ink-bar
      height 0
