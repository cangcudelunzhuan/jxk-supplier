.allinnermes
  width:100%
.firinner,.secinner,.thrinner,.zeroinner,.firstname
  width:100%
  background:#fff
  padding:24px
  padding-top:1px
  box-shadow:0px 0px 20px 0px rgba(231,197,211,0.24);
  border-radius:6px;
  margin-bottom:20px
.title_box
  margin-bottom: 10px

.form_box
  border-bottom: 1px solid #F0F2F5
  padding: 0 0 10px 0
  margin-bottom: 10px

.table_2
  width:100%
  border-left:1px solid #E8E8E8
  border-top:1px solid #E8E8E8
  border-right:1px solid #E8E8E8

  th,td
    padding:20px 15px
    border-bottom:1px solid #E8E8E8
  th
    width:15%
    text-align: right
    color:#333
    font-weight: normal
  td
    width:35%
    color:#999
  td:nth-of-type(1)
    border-right:1px solid #E8E8E8


.table_1
  width:100%
  border-left:1px solid #E8E8E8
  border-top:1px solid #E8E8E8

  th
    background-color:#FAFAFA
    text-align: right
    width:15%
  td
    width:35%

  th,td
    padding:20px 15px
    border-right:1px solid #E8E8E8
    border-bottom:1px solid #E8E8E8

.box_title
  margin-bottom:10px
  margin-top:35px
.btnstyle
  width:100%
  display: flex
  align-items: center
  justify-content: center
  margin-top:30px